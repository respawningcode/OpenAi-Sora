{
  "name": "sora-flows",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:dev": "dotenv -e .env.development next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "postbuild:dev": "dotenv -e .env.production next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint src",
    "prettier:check": "prettier src --check",
    "prettier:write": "prettier src --write",
    "prisma-dev:push": "dotenv -e .env.development prisma db push --schema=./prisma/schema.prisma",
    "prisma-dev:generate": "dotenv -e .env.development prisma generate",
    "prisma-pro:push": "dotenv -e .env.production prisma db push --schema=./prisma/schema.prisma",
    "prisma-pro:generate": "dotenv -e .env.production prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.1.0",
    "@prisma/client": "^5.10.2",
    "@tanstack/query-async-storage-persister": "^5.24.1",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@tanstack/react-query-persist-client": "^5.24.1",
    "@types/mdx": "^2.0.11",
    "ahooks": "^3.7.10",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.8",
    "markdown-to-jsx": "^7.4.1",
    "next": "^14.0.0",
    "next-auth": "^4.24.6",
    "next-intl": "^3.9.0",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^2.7.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-intersection-observer": "^9.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@iconify-json/mingcute": "^1.1.16",
    "@iconify/tailwind": "^0.1.4",
    "@types/dotenv": "^8.2.0",
    "@types/next": "^9.0.0",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "negotiator": "^0.6.3",
    "postcss": "^8",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.3"
  }
}
